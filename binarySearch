# Binary Search using Iteration

def binarySearchIter(arr, key):
    low = 0
    hi = len(arr) - 1
    while low <= hi:
        mid = (low + hi) // 2
        if arr[mid]==key:
            return mid
        if arr[mid] < key:
            low = mid + 1
        else:
            hi = mid - 1
    return -1

arr = [1,2,3,4,5,6,7,8]
ind = binarySearchIter(arr, 1)
print(ind)
          
